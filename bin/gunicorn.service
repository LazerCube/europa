[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=ubuntu
Group=ubuntu
WorkingDirectory=/home/ubuntu/myproject/myproject
ExecStart=/home/ubuntu/.bash_profile --workers 3 --bind unix:/home/ubuntu/myproject/myproject/myproject.sock config.wsgi:application

[Install]
WantedBy=multi-user.target
